            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".popplattegrondenCMGT"
            android:label="Open dag CMGT"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".popplattegrondenComm"
            android:label="Open dag Communicatie"
            android:theme="@style/AppTheme.CustomTheme" />
    </application>

</manifest>