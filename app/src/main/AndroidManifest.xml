<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androidhunger.opendagapp">
    <uses-permission android:name="android.permission.INTERNET"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/transparant"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity

            android:name=".Inschrijven"
            android:label="@string/title_activity_inschrijven"
            android:theme="@style/AppTheme.NoActionBar"/>



        <activity
            android:name=".Splashscreen"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".i"
            android:label="@string/I" />
        <activity
            android:name=".CMGT"
            android:label="@string/CMGT" />
        <activity
            android:name=".CMD"
            android:label="@string/CMD" />
        <activity
            android:name=".C"
            android:label="@string/c" />
        <activity
            android:name=".ti"
            android:label="@string/TI" />
        <activity
            android:name=".pophelp"
            android:label="@string/help"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Pop"
            android:label="Klik voor google maps"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".popplattegrondenInformatica"
